<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ptw.shiro.mapper.SysRoleMapper">

    <!-- 开启二级缓存 -->
    

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ptw.shiro.pojo.SysRole">
        <id column="id" property="id" />
        <result column="role" property="role" />
        <result column="description" property="description" />
        <result column="resource_ids" property="resourceIds" />
        <result column="available" property="available" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id AS id, role AS role, description AS description, resource_ids AS resourceIds, available AS available
    </sql>

</mapper>
